{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/react-in-jsx-scope": "off",
    "indent": [
      "error",
      2,
      { "SwitchCase": 1, "offsetTernaryExpressions": true }
    ],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-console": [
      "error",
      {
        "allow": ["error"]
      }
    ]
  }
}
